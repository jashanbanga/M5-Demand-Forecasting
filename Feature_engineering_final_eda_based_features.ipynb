    "#Moving Averages\n",
    "import tqdm\n",
    "\n",
    "window=[7,14,21,28,35]\n",
    "for i in tqdm.tqdm(window):\n",
    "    c3['rolling_mean_'+str(i)]=c3.groupby(['id'])['sales_units'].transform(lambda s: s.rolling(i).mean())\n",
    "\n",
    "\n",
    "window=['rolling_mean_7','rolling_mean_14', 'rolling_mean_21' ,'rolling_mean_28', 'rolling_mean_35']\n",
    "for i in window:\n",
    "    c3[i]=c3[i].fillna(0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "c3[\"diff\"] = c3.groupby(['id'])['sales_units'].transform(lambda s: s.diff()).shift(3049)\n",
    "c3[\"diff\"] = c3[\"diff\"].fillna(0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Lags\n",
    "lags = [1,7,14,21,28,35]\n",
