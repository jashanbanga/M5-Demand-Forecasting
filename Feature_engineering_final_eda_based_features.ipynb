    "for i in lags:\n",
    "    c3['lag_'+str(i)]=c3.groupby(['id'])['sales_units'].shift(i)\n",
    "# if you see in dataframe there is lot of NaN value get created. i am replacing NaN value with \"0\".\n",
    "lags=['lag_1','lag_7', 'lag_14','lag_21', 'lag_28', 'lag_35']\n",
    "\n",
    "for i in lags:\n",
    "    c3[i]=c3[i].fillna(0)"
   ]
